<form action="/change" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input type="text" name="branch" value="<%= @dojo.branch %>">
    <input type="text" name="street" value="<%= @dojo.street %>">
    <input type="text" name="city" value="<%= @dojo.city %>">
    <input type="text" name="state" value="<%= @dojo.state %>">
    <input type="hidden" name="id" value="<%= @dojo.id %>">
    <input type='submit' value='Edit'>
</form>


  <a href="/">Go Home</a>

  <a href="/show/<%= @dojo.id %>">Show</a>
