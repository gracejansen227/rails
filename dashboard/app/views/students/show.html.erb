
<h1><%= @student.first_name %> <%= @student.last_name %></h1>
<h3><%= @student.email %></h3>

<h2> Cohort </h2>
<table>
  <thead>
    <tr>
      <td>First Name</td>
      <td>Last Name</td>
      <td>Email</td>
    </tr>
  </thead>
  <tbody>

    <% @students.each do |student| %>
      <tr>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<a href="/back"><button>Home</button></a>
<a href="/dojos/<%= @student.dojo_id %>/students/<%= @student.id %>/edit"><button>Edit User</button></a>
