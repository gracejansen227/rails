<h1> <%= @dojo.branch %> </h1>
      Street:
      <%= @dojo.street %>
      City:
      <%= @dojo.city %>
      State:
      <%= @dojo.state %>
    </h4>

    <h1> Student Roster</h1>
    <table>
      <thead>
        <tr>
          <td>First Name</td>
          <td>Last Name</td>
          <td>Email</td>
          <td> Actions</td>
        </tr>
      </thead>
      <tbody>
      <% @students.each do |student| %>
        <tr>
          <td><%= student.first_name %></td>
          <td><%= student.last_name %></td>
          <td><%= student.email %></td>
          <td> <a href="/dojos/<%= student.dojo_id %>/students/<%= student.id %>">Show</a> <a href="/dojos/<%= student.dojo_id %>/students/<%= student.id %>/edit">Edit</a>
            <a data-method="delete" href="/dojos/<%= student.dojo_id %>/students/<%= student.id %>">Delete</a> </td>
        </tr>
      <% end %>
      </tbody>

      <a href="/new">Add a new branch</a>
    </table>

    <a href="/">Go Back Home</a>

    <a href="/dojos/<%= @dojo.id %>/students/new">Add a new student</a>

    <a href="/edit/<%= @dojo.id %>">Edit this Dojo </a>
