<form action="/dojos/<%= @student.dojo_id %>/students/<%= @student.id %>" method="post">
    <input type="hidden" name="_method" value="patch">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    First Name:
    <input type="text" name="first_name" value="<%= @student.first_name %>">
    Last Name:
    <input type="text" name="last_name" value="<%= @student.last_name %>">
    Email
    <input type="text" name="email" value="<%= @student.email %>">

    Dojo Branch:
    <select name="id">
  <option selected value="<%= @student.dojo_id %>"><%= @user_dojo.branch %></option>
  <% @dojos.each do |dojo| %>
  <option value="<%= dojo.id %>"><%= dojo.branch %></option>
  <% end %>
  </select>
    <input type="hidden" name="id" value="<%= @student.id %>">
    <input type='submit' value='Edit'>
</form>


  <a href="/">Go Home</a>

  <a href="/dojos/<%= @student.dojo_id %>/students/<%= @student.id %>">Show</a>
